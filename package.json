{
  "name": "react-inputs-validation",
  "version": "0.0.18",
  "description": "a react inputs validation component",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwardfhsiao/react-inputs-validation.git"
  },
  "keywords": [
    "react",
    "input",
    "inputs",
    "validate",
    "validation"
  ],
  "author": "Edward Xiao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/edwardfhsiao/react-inputs-validation/issues"
  },
  "homepage": "https://github.com/edwardfhsiao/react-inputs-validation#readme",
  "scripts": {
    "babel": "BABEL_ENV=lib babel ./src/js/Inputs --out-dir ./lib/components",
    "build": "rm -rf lib && npm run babel && npm run build_prod",
    "webpack_dev": "rm -rf dist && NODE_ENV=development ./node_modules/.bin/webpack --config ./webpack/development.config.js --progress --watch",
    "webpack_prod": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/production.config.js --progress -p",
    "build_dev": "npm run webpack_dev",
    "build_prod": "npm run webpack_prod",
    "build_lib_js": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/library.config.js",
    "build_lib_js_min": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/library.config.js --env build",
    "build_lib": "npm run build_lib_js & npm run build_lib_js_min && rm -rf lib/components && npm run babel ",
    "server_dev": "node_modules/.bin/webpack-dev-server --config ./webpack/development.config.js",
    "server_prod": "node_modules/.bin/webpack-dev-server --config ./webpack/production.config.js"
  },
  "dependencies": {
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^1.0.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.1.0",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "empty-module": "^0.0.2",
    "eslint": "^3.16.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.14.0",
    "node-sass": "^4.5.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-markdown": "^2.5.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-manifest-plugin": "^1.0.0"
  }
}
